<launch>
 <!-- socket node -->
  <node pkg="txi_launchpad" type="cc3200_joy" name="cc3200_joy">
    <param name="port" value="2390" />
	<param name="server_ip" type="string" value="192.168.0.114" />
  </node>

 <!-- joy node -->
  <node respawn="true" pkg="joy"
        type="joy_node" name="gru_joy" >
    <param name="dev" type="string" value="/dev/input/js0" />
    <param name="deadzone" value="0.50" />
  </node>

</launch>
